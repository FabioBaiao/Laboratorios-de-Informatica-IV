<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android" package="Mnham_Mnham.Mnham_Mnham" android:versionCode="1" android:versionName="1.0" android:installLocation="auto">
	<uses-sdk android:minSdkVersion="21" />
    
  <!-- A versão 2 do Google Maps para Android usa OpenGL ES 2.0. -->
	<uses-feature android:glEsVersion="0x00020000" android:required="true" />
	<!-- Permite que a aplicação aceda aos serviços web da Google. -->
	<uses-permission android:name="com.google.android.providers.gsf.permission.READ_GSERVICES" />
	<!-- A versão 2 do Google Maps para Android faz caching de partes de mapas no armazenamento externo. -->
	<uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />
	<!-- A versão 2 do Google Maps para Android precisa de confirmar que tem ligação de rede para descarregar mapas. -->
	<uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />
	<uses-permission android:name="android.permission.ACCESS_COARSE_LOCATION" />
	<uses-permission android:name="android.permission.ACCESS_FINE_LOCATION" />
	<!-- Permission to receive remote notifications from Google Play Services -->
	<!-- Notice here that we have the package name of our application as a prefix on the permissions. -->
	<!-- <uses-permission android:name="
    .permission.MAPS_RECEIVE" />
    <permission android:name=""<PACKAGE NAME="">
      .permission.MAPS_RECEIVE" android:protectionLevel="signature" />
  -->
	<!-- Permite a realização de pedidos por voz. -->
	<uses-permission android:name="android.permission.RECORD_AUDIO" />
	<uses-permission android:name="android.permission.INTERNET" />
	<application android:label="Mnham_Mnham" android:icon="@drawable/Icon"></application>
  <application android:label="@string/app_name">
    <meta-data android:name="com.google.android.geo.API_KEY" android:value="AIzaSyCZ14lwmScaCnXCAGJvkyIrGX4iKecKZvo" />
    <meta-data android:name="com.google.android.gms.version" android:value="@integer/google_play_services_version" />
  </application>
</manifest>
